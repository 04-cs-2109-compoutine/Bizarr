{
  "expo": {
    "name": "Bizarr",
    "slug": "Bizarr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bizarrm.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5C8D89"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },

    "description": "A marketplace to find everything you would ever need for a reasonable price.",
    "githubUrl": "https://github.com/04-cs-2109-compoutine/Bizarr"
  }
}
